{
  "plugins": [
    "stylelint-order"
  ],
  "processors": [
  ],
  "extends": [
    "stylelint-config-recommended",
  ],
  "rules": {
    "at-rule-no-unknown": [
      true, {
        "ignoreAtRules": ["each"]
      }
    ],
    "order/order": [
      "custom-properties",
      "declarations"
    ],
    "order/properties-alphabetical-order": true
  }
}
